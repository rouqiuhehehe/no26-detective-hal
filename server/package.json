{
    "name": "no26-detective-hal-server",
    "version": "1.0.4",
    "description": "26号探案馆后端项目",
    "main": "app.js",
    "typings": "./types/*.d.ts",
    "scripts": {
        "tsc": "babel src --out-dir dist --extensions \".ts,.js\"",
        "serve": "cross-env NODE_ENV=development NODE_RUN=dev nodemon -e ts,tsx --exec node --trace-warnings -r ./bin.js src/app.ts",
        "build": "node .bin/build",
        "debugger": "cross-env NODE_ENV=development NODE_RUN=debugger nodemon -e ts,tsx --exec node --inspect --expose-gc --trace-warnings -r ./bin.js src/app.ts",
        "test": "mocha -w --recursive -r ./bin.js ./test/**/*.spec.ts",
        "concurrent-test": "autocannon -c 1000 -p 5 http://127.0.0.1:1337/admin"
    },
    "pkg": {
        "assets": [
            "key/**/*",
            "uploads/**/*",
            "config/**/*"
        ],
        "scripts": [
            "dist/**/*.js"
        ],
        "outputPath": "build"
    },
    "bin": "dist/app.js",
    "author": "yoshiki",
    "license": "ISC",
    "browser": {
        "crypto": false
    },
    "preferGlobal": true,
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.5",
        "@babel/plugin-proposal-class-properties": "^7.16.5",
        "@babel/plugin-proposal-decorators": "^7.16.5",
        "@babel/preset-env": "^7.16.5",
        "@babel/preset-typescript": "^7.16.5",
        "@types/bcryptjs": "^2.4.2",
        "@types/chai": "^4.2.21",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.12",
        "@types/express-session": "^1.17.4",
        "@types/jsonwebtoken": "^8.5.4",
        "@types/mocha": "^9.0.0",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/mysql": "^2.15.19",
        "@types/node": "^16.0.0",
        "@types/redis": "^2.8.31",
        "@types/sinon": "^10.0.6",
        "@types/sinon-chai": "^3.2.6",
        "@types/uuid": "^8.3.4",
        "babel-plugin-module-resolver": "^4.1.0",
        "chai": "^4.3.4",
        "mocha": "^9.0.2",
        "morgan": "^1.10.0",
        "node-mocks-http": "^1.11.0",
        "prettier": "^2.3.1",
        "sinon": "^12.0.1",
        "sinon-chai": "^3.7.0",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "axios": "^0.24.0",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.5",
        "cross-env": "^7.0.3",
        "ejs": "^3.1.6",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "file-stream-rotator": "^0.5.7",
        "iconv-lite": "^0.4.24",
        "joi": "^17.4.1",
        "jsonwebtoken": "^8.5.1",
        "multer": "^1.4.4",
        "mysql": "^2.18.1",
        "mysqldump": "^3.2.0",
        "node-xlsx": "^0.21.0",
        "path-browserify": "^1.0.1",
        "redis": "^4.0.4",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^10.0.0",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.10.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "uuid": "^8.3.2"
    }
}
